# WebSocket Proxy URL
VITE_WS_URL=ws://localhost:8080

# Azure Speech Service
VITE_AZURE_SPEECH_KEY=your_azure_speech_key_here
VITE_AZURE_SPEECH_REGION=japaneast

# Azure OpenAI Service
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
VITE_AZURE_OPENAI_KEY=your_azure_openai_key_here
VITE_AZURE_OPENAI_DEPLOYMENT=gpt-4o  # Default deployment for Azure Service
VITE_AZURE_OPENAI_DEPLOYMENT_GPT4O=gpt-4o  # Deployment for Information Extraction

# Google Cloud Platform (for future OAuth implementation)
VITE_GCP_CLIENT_ID=your_gcp_client_id_here
VITE_GCP_PROJECT_ID=your_gcp_project_id_here

# Google OAuth Access Token (for OAuth Direct mode)
# You can get this using gcloud auth print-access-token
VITE_GOOGLE_ACCESS_TOKEN=your_google_access_token_here

# Ticket System Configuration
VITE_USE_TICKET_SYSTEM=true
VITE_DEPARTURE_STATION=水戸

# Gemini API Key (for information extraction)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Cosmos DB Configuration
VITE_COSMOS_ENDPOINT=https://jre-cosmos-je.documents.azure.com:443/
VITE_COSMOS_KEY=your_cosmos_key_here
